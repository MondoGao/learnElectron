  "license": "MIT",
  "scripts": {
    "dev": "yarn run dev:renderer",
    "dev:renderer": "cross-env NODE_ENV=develop BABEL_ENV=renderer node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config build/webpack.config.renderer.dev.js",
    "dev:main": "cross-env NODE_ENV=develop BABEL_ENV=main electron -r babel-register ./app/main.dev"
  },
  "browserlists": [
    "Electron 1.7"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-devtools-installer": "^2.2.1",
    "file-loader": "^1.1.5",
    "postcss": "^6.0.13",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "react-hot-loader": "^3.1.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "normalize.css": "^7.0.0",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
